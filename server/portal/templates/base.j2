    <!-- <script src="{% static 'vendor/angular-aria/angular-aria.min.js' %}"></script> -->
    <!-- <script src="{% static 'vendor/angular-material/angular-material.min.js' %}"></script> -->
    <!-- <script src="{% static 'vendor/angular-messages/angular-messages.min.js' %}"></script> -->
    <!-- <script src="{% static 'vendor/angular-animate/angular-animate.min.js' %}"></script> -->
    <!-- <script src="{% static 'vendor/ng-storage/ngStorage.js' %}"></script> -->
    <!-- <script src="{% static 'vendor/angular-cookies/angular-cookies.min.js' %}"></script> -->
    <!-- <script src="{% static 'vendor/angular-ui-router/release/angular-ui-router.min.js' %}"></script> -->
    <!-- <script src="{% static 'vendor/tv4/tv4.js' %}"></script> -->
